programa 
{
    funcao inicio() 
    {
        // Declarção de Variáveis
        real dividendo
        real divisor
        real resultado

        //Entrada de Dados do Usuário
        escreva("Digite o Dividendo: ")
        leia(dividendo)
        escreva("Digite o Divisor: ")
        leia(divisor)
        // Saida de dados emitindo o resultado
        resultado = dividendo / divisor
        escreva (resultado)
    }
}